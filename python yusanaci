from tkinter import *
root=Tk()
root.title("yusanci")
root.geometry("800x600")
label_series=Label(root,text="yusanaci series : ")
label_flower=Label(root)
label_spirl=Label(root)

def teranci():
    num=50
    firstnum=0
    secnum=1
    sum=0
    counter=1
    while(counter<=num):
        label_series["text"]+=str(sum)+" "
        counter=counter+1
        firstnum=secnum
        secnum=sum
        sum=firstnum+secnum
        label_flower['text']="flower is still blooming"
        label_spirl['text']="spirl in right direction are"+str(firstnum)+"and spirl in left are"+str(secnum)+"\n. total spirl are"+str(sum)
       
        btn=Button(root,text="show yusanaci series",command=teranci)
        btn.pack()
       
        label_flower.pack()
        label_series.pack()
        label_spirl.pack()
        get_question_answered=["3.4",35,"3.5"+35]
        print(get_question_answered)
        get_question_answer.pack()

root.mainloop()
